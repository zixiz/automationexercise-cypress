{
  "compilerOptions": {
    "target": "es2018",
    "lib": ["es2018", "dom"],
    "types": ["cypress", "node"],
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "isolatedModules": false,
    "baseUrl": "./",
    "paths": {
      "@pageObjects/*": ["cypress/pages/*"],
      "@support/*": ["cypress/support/*"],
      "@fixtures/*": ["cypress/fixtures/*"],
      "@utils/*": ["cypress/utils/*"]
    }
  },
  "include": [
    "node_modules/cypress",
    "cypress/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
} 